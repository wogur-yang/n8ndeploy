{
	"compilerOptions": {
		"strict": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"target": "es2019",
		"lib": ["es2019", "es2020", "es2022.error"],
		"removeComments": true,
		"useUnknownInCatchVariables": false,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		 "skipLibCheck": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"strictNullChecks": true,
		"preserveConstEnums": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"incremental": true,
		"declaration": true,
		"sourceMap": true,
		"outDir": "./dist/",
		"allowJs": true,
    	"baseUrl": "./",
    	"paths": {
      		"youtube-transcript_custom/*": ["../youtube-transcript_custom/*"]
    	},
		"typeRoots": ["./node_modules/@types"]
	},
	"include": [
		"credentials/**/*",
		"nodes/**/*",
		"nodes/**/*.json",
		"package.json",
		"index.ts",
	],
}
